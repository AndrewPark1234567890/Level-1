
public class PeopleMaker {
public static void main(String[] args) {
	Person Andrew = new Person("Andrew",
			"Ability to make a peace sign and teleport anywhere he wants :D",
			false);
	Person Phil = new Person("Phil", "Ability to survive any disease", true);
	Person Eli = new Person("Eli", "None?", false);
	Person Slave = new Person("Minion241", "Ability to serve Andrew and be invincible", false);
	Person Olivia = new Person("Olivia", "Flight + tek stuff", false);
	Person Slave2 = new Person("Minion358", "Ability to serve Andrew and be invincible", false);
	Person Loser = new Person("Loserface", "Annoyance", true);
	Person June = new Person("June", "Ablility to confuse people with handwriting", false);
	System.out.println(Phil);
}
}
